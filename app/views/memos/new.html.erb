<div id="contact" class="big-bg">
     <header class="page-header wrapper">
        <h1><%= link_to image_tag("logo.png", class: :logo), root_path %></h1>
        <nav>
            <ul class="main-nav">
                <li><a href="#">ABOUT</a></li>
                <li><a href="#">SERVICE</a></li>
                <li><a href="#">CONTACT</a></li>
            </ul>
        </nav>
    </header>

  <div class="wrapper">
    <h2 class="page-title">NEW MEMO</h2>
    <form>
        <%= form_with model: @memo, local: true do |f|%>
          <div class="field">
            <%= f.label :heartstring, class: :label %>
            <%= f.text_area :heartstring %>
          </div>

          <div class="field">
            <%= f.label :name, class: :label %>
            <%= f.text_field :name %>
          </div>

          <div class="action">
            <%= f.submit "Submit", class: :button %>
          </div>
          <% end %>
    </form>  
  </div>
</div>